[
  {
     "description": "重定向到百度",
     "request": {
      "uri":"/redirect",
      "method": "get"
    },
    "redirectTo": "https://www.baidu.com"

 },

  {
    "description": "重定向到自己的网页上",
    "request": {
      "uri":"/redirect1",
      "method": "get"
    },
    "redirectTo": "/redirect/new"

  },

  {
    "description": "这是被重定向的一个请求",
    "request": {
      "uri": "/redirect/new",
      "method": "get"
    },
    "response": {
      "headers": {
        "content-Type": "text/html;charset=gbk"
      },
      "text": "重定向成功啦"
    }


  }

]